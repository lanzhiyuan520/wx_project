.cut-off{
  height: 10rpx;
  background: #fff5f9;
}
/*月嫂资料  */
.plr-30-box{
  padding:50rpx 60rpx 30rpx; 
}
.nanny-pic{
  width: 167rpx;
  height: 167rpx;
  margin-right: 35rpx;
}
.nanny-info-box{
  display: flex;
  flex-direction: row;
  position: relative;
}
.share{
  position: absolute;
  top: 0;
  right: 0;
  padding: 0;
  margin: 0;
  background-color: #fff;
  line-height: 40rpx;
}
.share::after{
  border: none;
  border-radius: 0;
}
.share image{
  width: 41rpx;
  height: 39rpx;
}
.nanny-info{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.intro{
  line-height: 40rpx;
  margin-top: 45rpx;
}
.text-show-btn{
  margin-left: 60rpx
}
.extend{
  height: auto
}
/*印象标签  */
.impress-lable{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 35rpx;
  line-height: 40rpx;
}
.mr-30{
  margin-right: 30rpx;
}
.model-title{
  font-size: 34rpx;
  font-weight: bold;
}
.ptb-35-box{
 padding:35rpx 60rpx 30rpx; 
}
/*保障区  */
.safe-pic-box{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.safe-pic{
  width: 110rpx;
  height: 74rpx;
  margin-bottom: 30rpx;
}
.ptb-55-box{
 padding:60rpx 60rpx 30rpx; 
}
.safe-pic-item{
  display: flex;
  flex-direction: column;
  align-items: center;
}
/*工作照片  */
.job-pic{
  width: 230rpx;
  height: 210rpx;
  border-radius: 10rpx;
  margin-right: 40rpx;
}
.job-pic:last-child{
  margin-right: 0;
}
/*客户点评  */
.customer-pic{
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-right: 35rpx;
}
.start-pic{
  width: 20rpx;
  height: 20rpx;
  margin-right: 13rpx;
}
.start-box{
  display: flex;
  flex-direction: row;
}
.customer-info{
  display: flex;
  flex-direction: column;
  justify-content: space-around
}
.review-item{
  padding: 35rpx 0 40rpx;
}
.review-text{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.line-50{
  line-height: 50rpx;
}
.ptb-25{
  padding: 25rpx 0;
}
.review-pic{
  width: 145rpx;
  height: 114rpx; 
  border-radius: 10rpx;
  margin-right: 40rpx;
  margin-bottom: 20rpx;
}
.review-pic-box{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.more-btn{
  width: 135rpx;
  height: 40rpx;
  border: 1rpx solid #b5b5b5;
  border-radius: 10rpx;
  line-height: 30rpx;
  text-align: center
}
.btn-box{
  display: flex;
  justify-content: center;
}
/*遮罩  */
.mask{
  position: fixed;
  background: rgba(0, 0, 0, 1);
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 9999;
}
.slide-image{
  width: 100%;
}
.mask-pic-box{
  width: 100%;
  height: 100%;
}
.center-pic{
  display: flex;
  justify-content: center; 
  align-items: center;
}
.pl-60{
 padding-left:60rpx; 
}
.like-waiter{
  padding: 35rpx 0 150rpx;
}